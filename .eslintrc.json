{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "no-plusplus": 0,
    "react/no-array-index-key": 1,
    "comma-dangle": ["error", {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "never"
    }]
  }
}